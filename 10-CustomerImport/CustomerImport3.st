- -- -- -- -- -- -- -- -- -- -- -- "!IntegrationEnvironment class	instanceVariableNames: ''!!IntegrationEnvironment class methodsFor: 'consulting' stamp: 'NicolasPazos 6/27/2018 23:33'!isCurrent	^false! !CustomerSystem subclass: #PersistentCustomerSystem	instanceVariableNames: 'session'	classVariableNames: ''	poolDictionaries: ''	category: 'IS1-CustomerImport-Model'!!PersistentCustomerSystem methodsFor: 'accessing' stamp: 'NicolasPazos 6/27/2018 21:18'!customerIdentifiedAs: anIdentifycationType numbered: anIdentificationNumber ifNone: aBlockToEvalauteOnFailure		|customers |		customers := session 		select: [ :aCustomer | aCustomer identificationType = anIdentifycationType and: [ aCustomer identificationNumber = anIdentificationNumber ] ]		ofType: Customer.			customers size = 1 ifFalse: [ ^aBlockToEvalauteOnFailure value].		^customers anyOne.! !!PersistentCustomerSystem methodsFor: 'accessing' stamp: 'NicolasPazos 6/27/2018 21:18'!allCustomers	^ session selectAllOfType: Customer! !!PersistentCustomerSystem methodsFor: 'importing' stamp: 'NicolasPazos 6/27/2018 21:47'!add: anObjectToAddToTheSystem	session persist: anObjectToAddToTheSystem! !!PersistentCustomerSystem methodsFor: 'opening - closing' stamp: 'NicolasPazos 6/27/2018 21:17'!close	session close! !!PersistentCustomerSystem methodsFor: 'opening - closing' stamp: 'NicolasPazos 6/27/2018 21:17'!commit	session commit! !!PersistentCustomerSystem methodsFor: 'opening - closing' stamp: 'NicolasPazos 6/27/2018 21:17'!start	session open! !!PersistentCustomerSystem methodsFor: 'opening - closing' stamp: 'NicolasPazos 6/27/2018 21:18'!beginTransaction	session beginTransaction! !!PersistentCustomerSystem methodsFor: 'initialization' stamp: 'NicolasPazos 6/27/2018 21:04'!initialize											self initializeSession ! !!PersistentCustomerSystem methodsFor: 'initialization' stamp: 'NicolasPazos 6/27/2018 21:03'!initializeSession	session:=			(DataBaseSession				for:					(Array						with:							(ClassMapping								withDefaultTableNameFor: Address								mappingAll:									(Array										with: (EmbededMapping withDefaultFieldNameFor: #streetName)										with: (EmbededMapping withDefaultFieldNameFor: #streetNumber)										with: (EmbededMapping withDefaultFieldNameFor: #town)										with: (EmbededMapping withDefaultFieldNameFor: #zipCode)										with: (EmbededMapping withDefaultFieldNameFor: #province)))						with:							(ClassMapping								withDefaultTableNameFor: Customer								mappingAll:									(Array										with: (EmbededMapping withDefaultFieldNameFor: #firstName)										with: (EmbededMapping withDefaultFieldNameFor: #lastName)										with: (EmbededMapping withDefaultFieldNameFor: #identificationType)										with: (EmbededMapping withDefaultFieldNameFor: #identificationNumber)										with: (OneToManyMapping withDefaultFieldNameFor: #addresses ofType: Address)))))! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PersistentCustomerSystem class	instanceVariableNames: ''!!PersistentCustomerSystem class methodsFor: 'instance creation' stamp: 'NicolasPazos 6/27/2018 21:04'!new	^self basicNew initialize! !CustomerSystem subclass: #TransientCustomerSystem	instanceVariableNames: 'customers'	classVariableNames: ''	poolDictionaries: ''	category: 'IS1-CustomerImport-Model'!!TransientCustomerSystem methodsFor: 'accessing' stamp: 'NicolasPazos 6/27/2018 21:32'!customerIdentifiedAs: anIdentifycationType numbered: anIdentificationNumber ifNone: aBlockToEvalauteOnFailure		^customers detect: [ :aCustomer | aCustomer isIdentifiedAs: anIdentifycationType numbered: anIdentificationNumber ] ifNone: aBlockToEvalauteOnFailure .! !!TransientCustomerSystem methodsFor: 'accessing' stamp: 'NicolasPazos 6/27/2018 21:29'!allCustomers	^customers copy! !!TransientCustomerSystem methodsFor: 'importing' stamp: 'NicolasPazos 6/27/2018 21:47'!add: anObjectToAddToTheSystem	customers add: anObjectToAddToTheSystem ! !!TransientCustomerSystem methodsFor: 'initialization' stamp: 'NicolasPazos 6/27/2018 21:29'!initialize	customers := OrderedCollection new.! !!TransientCustomerSystem methodsFor: 'opening - closing' stamp: 'NicolasPazos 6/27/2018 21:59'!beginTransaction! !!TransientCustomerSystem methodsFor: 'opening - closing' stamp: 'NicolasPazos 6/27/2018 21:58'!start! !!TransientCustomerSystem methodsFor: 'opening - closing' stamp: 'NicolasPazos 6/27/2018 21:59'!close! !!TransientCustomerSystem methodsFor: 'opening - closing' stamp: 'NicolasPazos 6/27/2018 21:58'!commit
! !